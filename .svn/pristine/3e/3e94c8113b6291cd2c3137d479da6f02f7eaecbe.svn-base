<div class="table col-sm-12">
    <div class="col-sm-12 plan-title text-center">
      <div class="col-sm-3">门店</div>
      <div class="col-sm-3">发送数量</div>
      <div class="col-sm-3">金额</div>
      <div class="col-sm-2">明细</div>
    </div>

    <div class="col-sm-12 plan-son text-center slide-down" ng-repeat="row in view.result.items">
      <div class="col-sm-3" ng-bind="row.shopName"></div>
      <div class="col-sm-3" ng-bind="row.count|number"></div>
      <div class="col-sm-3" ng-bind="row.amount|number"></div>
      <div class="col-sm-2" >
        <button class="btn btn-primary" ng-click="showDetail(row.shopId)">查看明细</button>
      </div>
    </div>
</div>
