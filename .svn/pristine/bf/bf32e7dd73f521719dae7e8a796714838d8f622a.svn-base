<div class="v-shadow">
  <label class="left-tag" style="margin: 20px 0">营销素材</label>

  <div class="list-group click">
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='coupon'}" ng-click="goto('rule/coupon')">券管理</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='allocate'}" ng-click="goto('rule/allocate')">分账规则</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='reward'}" ng-click="goto('rule/reward')"  ng-if="config.permission.includes('REWARD')">代用币管理</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='ads'}" ng-click="goto('rule/ads')">广告管理</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='material'}" ng-click="goto('rule/material')">图片管理</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='ueditor'}" ng-click="goto('rule/ueditor')">图文管理</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='video'}" ng-click="goto('rule/video')">视频管理</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='source'}" ng-click="goto('rule/source')">海报管理</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='gratuities'}" ng-click="goto('rule/gratuities')"  ng-if="config.permission.includes('GRATUITY')">打赏礼物管理</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='tags'}" ng-click="goto('rule/tags')" ng-if="config.permission.includes('GRATUITY')">评价标签管理</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='nonParticipation'}" ng-click="goto('rule/nonParticipation')">不参与项</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='cards'}" ng-click="goto('rule/cards')" ng-if="config.permission.includes('STORECARD')">储值卡管理</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='tasks'}" ng-click="goto('rule/tasks')">任务管理</a>
  </div>
  <label class="left-tag" style="margin: 20px 0">营销活动</label>

  <div class="list-group click">
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='front'}" ng-click="goto('rule/front')">首页广告管理</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='rule'}"  ng-click="goto('rule')">线上活动</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='ruleConsume'}"  ng-click="goto('ruleconsume')">消费活动</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='exhibition'}" ng-click="goto('rule/exhibition')" ng-if="config.permission.includes('IRREGULAR')">纯展示活动</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='member'}" ng-click="goto('rule/member')">会员管理</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='bargain'}" ng-click="goto('rule/bargain')" ng-if="config.permission.includes('BARGAIN')">买单砍价</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='groupon'}" ng-click="goto('rule/groupon')" ng-if="config.permission.includes('GROUPON')">在线砍价</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='comment'}" ng-click="goto('rule/comment')" ng-if="config.permission.includes('PRAISE')">评赏管理</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='lottery'}" ng-click="goto('rule/lottery')" ng-if="config.permission.includes('LOTTERY')">抽奖管理</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='gratuity'}" ng-click="goto('rule/gratuity')" ng-if="config.permission.includes('GRATUITY')">打赏管理</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='reserve'}" ng-click="goto('rule/reserve')" ng-if="config.permission.includes('RESERVE')">预定管理</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='commission'}" ng-click="goto('rule/commission')" ng-if="config.permission.includes('COMMISION')">返佣管理</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='redpackets'}" ng-click="goto('rule/redpackets')" ng-if="config.permission.includes('PACKET')">红包管理</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='referral'}" ng-click="goto('rule/referral')" ng-if="config.permission.includes('SALARY')">转介绍管理</a>
    <a class="list-group-item" ng-class="{'active':config.breadset[0].class=='share'}" ng-click="goto('rule/share')" ng-if="config.permission.includes('REBATE')">分享金</a>

  </div>



</div>
