<div class="container contents bgw ">
	<div class="col-sm-8  col-sm-offset-2">
		<form class="form-horizontal " ng-submit="send()">
			<div class="form-group">
				<label class="control-label col-sm-4"></label>
				<div class="col-sm-4">
					<div class="avatar-lg">
						<img ng-src="{{view.user.avatarUrl||'public/img/avatar.png'}}"></div>
				</div>
			</div>
			<div class="form-group">
				<label class="control-label col-sm-4">姓名</label>
				<div class="col-sm-4 ">
					<div class="form-control-static" ng-bind="view.user.nickname"></div>
				</div>
			</div>
			<div class="form-group">
				<label class="control-label col-sm-4">卡号</label>
				<div class="col-sm-4">
					<div class="form-control-static" ng-bind="view.user.memberCardNo"></div>
				</div>
			</div>
			<div class="form-group">
				<label class="control-label col-sm-4">级别</label>
				<div class="col-sm-4">
					<div class="form-control-static" ng-bind="view.user.memberGradeName"></div>
				</div>
			</div>
			<div class="form-group ">
				<label class="control-label col-sm-4"><span class="text-red">*</span>级别</label>

				<div class="col-sm-4">
					<select class="form-control" name="memberGradeId" ng-model="posts.memberGradeId" required>
						<option ng-repeat="item in view.member" ng-value="item.id">{{item.name}}

						</option>
						<!--<options="item.id as item.name for item in view.member" >-->

					</select>
					<!--<select type="date" class="form-control"  ng-model="posts.memberGradeId" >-->
					<!--<option value="member.grade" ng-repeat="member in view.member" >{{member.name}}</option>-->
					<!--</select>-->
					<!--<div class="form-control-static" ng-bind="posts.birthday"></div>-->
				</div>
			</div>

			<div class="form-group">
				<div class="col-sm-2 col-sm-offset-4">
					<!-- <a class="btn btn-default btn-block" back-button>返回</a> -->
				</div>
				<div class="col-sm-2 ">
					<button class="btn btn-primary btn-block" >确定</button>
				</div>
				
			</div>
		</form>
	</div>
</div>