<div class="table col-sm-12">
    <div class="col-sm-12 plan-title text-center">
      <div class="col-sm-1">序号</div>
      <div class="col-sm-2">顾客</div>
      <div class="col-sm-1">发放</div>
      <div class="col-sm-1">兑换</div>
      <div class="col-sm-2">原因</div>
      <div class="col-sm-2">操作员</div>
      <div class="col-sm-3">时间</div>
    </div>

    <div class="col-sm-12 plan-son text-center slide-down" ng-repeat="row in result.items">
      <div class="col-sm-1" ng-bind="((page-1)*count)+$index+1"></div>
      <div class="col-sm-2" ng-bind="row.customerName"></div>
      <div class="col-sm-1" ng-bind="row.amount|number"></div>
      <div class="col-sm-1" ng-bind="row.usedAmount|number"></div>
      <div class="col-sm-2" ng-bind="row.cause"></div>
      <div class="col-sm-2" ng-bind="row.staffName"></div>
      <div class="col-sm-3" ng-bind="row.creatTime"></div>
    </div>
</div>
