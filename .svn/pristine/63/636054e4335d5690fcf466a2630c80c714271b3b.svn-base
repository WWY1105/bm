<div class="col-sm-12">
  <div class="form-group ml2">
    <div class="col-sm-3" ng-repeat="item in specialDateRangeCategory">
        <a class="btn btn-default" ng-class="{'btn-danger':specialDate.subType===item.type}"  ng-click="clickBtn(item.type)" ng-bind="item.name"></a>
    </div>
  </div>

  <div class="animate-switch-container" ng-switch on="specialDate.subType">
    <div class="animate-switch" ng-switch-when="MANUAL_SELECT" >
        <div class="row mt15 datebg">
          <div class="col-sm-6">
            <div class="datelist" ng-hide="tem.dateun">
              <div class="controle">
                <div class="col-sm-3 text-center">
                  <div class="row">
                    <div ng-click="datego(-1)"  class="btn btn-default btn-sm" ng-class="{'disabled':dare.i==0}"> <i class="glyphicon glyphicon-chevron-left"></i>
                    </div>
                  </div>
                </div>

                <div class="col-sm-6 text-center">
                  <div  class="row">
                    <span class="datecontrol" ng-bind="date.year+'年'+date.month+'月'"></span>
                  </div>

                </div>
                <div class="col-sm-3 text-center">
                  <div class="row">
                    <div ng-class="{'disabled':dare.i==0}"  ng-click="datego(1)"  class="btn btn-default btn-sm"> <i class="glyphicon glyphicon-chevron-right"></i>
                    </div>
                  </div>
                </div>

              </div>
              <div class="weektitle">
                <div class="weekson" ng-repeat="week in date.week" ng-bind="week"></div>
              </div>
              <div class="datecontent">
                <div class="dateout">
                  <div  ng-repeat="dates in date.dates" >
                    <div class="dateson" ng-class="{'bordert':$index<7,'borderr':($index+1)%7==0}" ng-if="dates.day">
                      <a ng-class="{'active':specialDate.selectDates[dates.ture],'bordert':$index<8,'borderr':($index+1)%7==0}" ng-click="clickDay(dates.ture, specialDate.selectDates)" ng-bind="dates.day" ng-if="!dates.color||specialDate.selectDates[dates.ture]"></a>
                      <span ng-class="{'active':specialDate.selectDates[dates.ture],'bordert':$index<8,'borderr':($index+1)%7==0}" style="color:#ddd" ng-bind="dates.day" ng-if="dates.color&&!specialDate.selectDates[dates.ture]"></span>
                    </div>

                    <div class="dateson"  ng-class="{'bordert':$index<7,'borderr':($index+1)%7==0}"  ng-if="!dates.day"></div>
                  </div>
                </div>
              </div>
            </div>

          </div>
          <div class="col-sm-6">
            <div class="col-sm-5 mt20" ng-repeat="(key, value) in specialDate.selectDates" ng-if="$index<11">
              <span ng-bind="key" class="label label-danger " ng-if="$index<10"></span>
              <span ng-if="$index==10">更多日期请查看列表</span>
            </div>
          </div>

        </div>
    </div>

    <div class="animate-switch" ng-switch-when="MONTH_DAYS">
      <div class="form-group col-sm-12 datebg">
        <label class="checkbox-inline" ng-repeat="ele in range(1, 31)"
        ng-class="{ml15:$index===0}" ng-style="{width:'40px'}">
        <input type="checkbox" ng-model="specialDate.selectDays[ele]">
        <span ng-bind="ele"></span>
      </label>
    </div>
  </div>

    <div class="animate-switch" ng-switch-when="WEEKLY_DAY">
      <div class="form-group mt15">
        <div class="col-sm-12 datebg ">
          <label class="checkbox-inline" ng-repeat="item in week" ng-class="{ml15:$index===0}" ng-click="clickTest(item)">
            <input type="checkbox" ng-model="specialDate.selectWeekDays[item.idx]">
            <span ng-bind="item.value"></span>
          </label>
        </div>
      </div>
    </div>

  </div>
</div>
