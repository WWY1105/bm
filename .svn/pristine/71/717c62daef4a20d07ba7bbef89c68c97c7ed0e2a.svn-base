	<div class="out ruleadd2" >
		<div class="col-sm-2 text-right ">
			<span class="title">活动详情</span>
		</div>
		<div class="col-sm-10 content mb0">
			<div class="col-sm-12 ml25 mb22 form-control-stack text-left">
				<label class="checkbox-inline">
					<input type="checkbox"  ng-model="posts.CHARGE.CHARGE_PREMENT.check" ng-checked="posts.CHARGE.CHARGE_PREMENT.check">充值送金额</label>
			</div>
			<div    ng-repeat="(key, detail) in posts.CHARGE.CHARGE_PREMENT.detail" ng-show="posts.CHARGE.CHARGE_PREMENT.check" >

				<div class="form-group mb22"  ng-repeat="details in detail" ng-init="details.id=key">
					<div class="col-sm-3" >
						<select ng-model="details.id"   class="form-control " 
  					ng-options="item.id as item.name for item in view.member" name="member" required></select>
					</div>

					<div class=" form-control-static pull-left">充值</div>
					<div class="col-sm-3 ">
						<input ng-model="details.amount" type="number" class="form-control" min ="1"/>	
					</div>
					<div class=" form-control-static pull-left">元，送</div>
					<div class="col-sm-3 ">
						<input ng-model="details.value" type="number" class="form-control" min ="1"/>	
					</div>
					<div class="pull-left form-control-static">元</div>
					<a class="add click" ng-if="$parent.$first&&$first" ng-click="ruleFn.addFn('CHARGE_PREMENT')"> <i class="glyphicon glyphicon-plus"></i>
					</a>
					<a class="remove click" ng-if="!$first||!$parent.$first" ng-click="ruleFn.removeFn('CHARGE_PREMENT',key,$index)"> <i class="glyphicon glyphicon-remove"></i>
					</a>
				</div>

			</div>
		</div>

	</div>
	<div class="out ruleadd2" >
		<div class="col-sm-2 text-right "></div>
		<div class="col-sm-10 content mb0">
			<div class="col-sm-12 ml25 mb22 form-control-stack text-left">
				<label class="checkbox-inline">
					<input type="checkbox"  ng-model="posts.CHARGE.CHARGE_POINT.check" ng-checked="posts.CHARGE.CHARGE_POINT.check">充值送积分</label>
			</div>
			<div   ng-repeat="(key, detail) in posts.CHARGE.CHARGE_POINT.detail" ng-show="posts.CHARGE.CHARGE_POINT.check" >
				<div  class="form-group mb22" ng-repeat="details in detail" ng-init="details.id=key">

					<div class="col-sm-3">
						<select ng-model="details.id"   class="form-control " 
  					ng-options="item.id as item.name for item in view.member" name="member" required></select>
					</div>
					<div class=" form-control-static pull-left">充值</div>
					<div class="col-sm-3 ">
						<input ng-model="details.amount" type="number" class="form-control" min ="1"/>	
					</div>
					<div class=" form-control-static pull-left">元，送</div>
					<div class="col-sm-2 ">
						<input ng-model="details.value" type="number" class="form-control" min ="1"/>	
					</div>
					<div class="pull-left form-control-static">积分</div>
					<a class="add click" ng-if="$parent.$first&&$first" ng-click="ruleFn.addFn('CHARGE_POINT')">
						<i class="glyphicon glyphicon-plus"></i>
					</a>
					<a class="remove click" ng-if="!$first||!$parent.$first" ng-click="ruleFn.removeFn('CHARGE_POINT',key,$index)">
						<i class="glyphicon glyphicon-remove"></i>
					</a>
				</div>
			</div>
		</div>

	</div>
	<div class="out ruleadd2" >
		<div class="col-sm-2 text-right "></div>
		<div class="col-sm-10 content mb0">
			<div class="col-sm-12 ml25 mb22 form-control-stack text-left" >

				<label class="checkbox-inline">
					<input type="checkbox"  ng-model="posts.CHARGE.CHARGE_COUPON.check" ng-change="console(posts.CHARGE.CHARGE_COUPON.check)" ng-init="posts.CHARGE.CHARGE_COUPON.detail||(posts.CHARGE.CHARGE_COUPON.detail=temobj)" ng-checked="posts.CHARGE.CHARGE_COUPON.check">充值送券</label>
			</div>

			<div    ng-repeat="(key, detail) in posts.CHARGE.CHARGE_COUPON.detail"  ng-show="posts.CHARGE.CHARGE_COUPON.check" >
				<div class="form-group mb22" ng-repeat="details in detail" ng-init="details.id=key">

					<div class="col-sm-3">
						<select ng-model="details.id"   class="form-control " 
  					ng-options="item.id as item.name for item in view.member" name="member" required></select>
					</div>
				<div class=" form-control-static pull-left">充值</div>
				<div class="col-sm-2 ">
					<input type="text" class="form-control" ng-model="details.amount" />	
				</div>
				<div class=" form-control-static pull-left">元，送</div>

				<div class="col-sm-2 " ng-show="view.coupons">
					<div class="row">
						<select class="form-control coupon-select" ng-change="ruleFn.addCouponPlanFn(details.value)" ng-model="details.value" ng-options="item._id as item.name group by view.counponType[item.type] for item in view.coupons"  >
						<option value="">请选择</option>

					</select>
					</div>
					
				</div>
				<div class="col-sm-2 " ng-show="!view.coupons">
					<A class="btn btn-default" ng-click="addCoupon()">点击添加券</A>
				</div>
				<div class="col-sm-2" >
					<input type="text" class="form-control" ng-model="details.count"/>	
				</div>
				<div class="pull-left form-control-static">张</div>

				<a class="add click" ng-if="$parent.$first&&$first" ng-click="ruleFn.addFn('CHARGE_COUPON')">
					<i class="glyphicon glyphicon-plus"></i>
				</a>
				<a class="remove click" ng-if="!$first||!$parent.$first" ng-click="ruleFn.removeFn('CHARGE_COUPON',key,$index)">
					<i class="glyphicon glyphicon-remove"></i>
				</a>
			</div>
			</div>
		</div>

	</div>
	<div class="out ruleadd2" >
		<div class="col-sm-2 text-right "></div>
		<div class="col-sm-10 content2 mb0">
			<div class="col-sm-12 ml25 mb22 form-control-stack text-left">
				<label class="checkbox-inline">
					<input type="checkbox"  ng-model="posts.CHARGE.CHARGE_GIFT.check" ng-checked="posts.CHARGE.CHARGE_GIFT.check">充值送礼</label>
			</div>
			<div    ng-repeat="(key, detail) in posts.CHARGE.CHARGE_GIFT.detail " ng-show="posts.CHARGE.CHARGE_GIFT.check" >
				<div class="form-group mb22" ng-repeat="details in detail" ng-init="details.id=key">

					<div class="col-sm-3">
						<select ng-model="details.id"   class="form-control " 
  					ng-options="item.id as item.name for item in view.member" name="member" required></select>
					</div>
				<div class=" form-control-static pull-left">充值</div>
				<div class="col-sm-2 ">
					<input type="text" class="form-control" ng-model="details.amount" />	
				</div>
				<div class=" form-control-static pull-left">元，送</div>
				<div class="col-sm-3 ">
					<input type="text" class="form-control" ng-model="details.value"/>	
				</div>
				<a class="add click" ng-if="$parent.$first&&$first" ng-click="ruleFn.addFn('CHARGE_GIFT')">
					<i class="glyphicon glyphicon-plus"></i>
				</a>
				<a class="remove click" ng-if="!$first||!$parent.$first" ng-click="ruleFn.removeFn('CHARGE_GIFT',key,$index)">
					<i class="glyphicon glyphicon-remove"></i>
				</a>
			</div>
			</div>
		</div>

	</div>