<form class="form-inline mt15 mb15" ng-init="init()">
  <div class="form-group" ng-if="shops">
    <label for="selectorShopId">门店</label>
    <select class="form-control" ng-model="posts.shopId" ng-options="o.id as o.name for o in shops" name="selectorShopId" ng-change="changeShopId()"></select>
  </div>
  <!--<div class="form-group">-->
    <!--<label for="order">订单号</label>-->
    <!--<input id="order" type="text" ng-model="no" class="form-control" name="no">-->
  <!--</div>-->
  <div class="form-group">
    <label for="startDate">开始日期</label>
    <input type="date" ng-model="starttime" class="form-control" name="startDate">
  </div>
  <div class="form-group">
    <label for="endDate">结束日期</label>
    <input type="date" ng-model="endtime" class="form-control" name="endDate">
  </div>
  <div class="form-group">
    <button class="btn btn-default btn-primary" ng-click="doSearch()">查询</button>
  </div>
</form>
