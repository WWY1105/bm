					<input ng-model="details.value" type="number" class="form-control" placeholder="请输入赠送额度" min ="0"/>
					<span class="form-control-static">元</span>
					<div class="form-group">
						<a class="add click" ng-if="$parent.$first&&$first" ng-click="ruleFn.addFn('CHARGE_PREMENT')"> <i class="glyphicon glyphicon-plus"></i>
						</a>
						<a class="remove click" ng-if="!$first||!$parent.$first" ng-click="ruleFn.removeFn('CHARGE_PREMENT',key,$index)"> <i class="glyphicon glyphicon-remove"></i>
						</a>
					</div>
				</div>
				<div class="form-group ml25 mb22" >
						<div class="form-inline">
							<span class="form-control-static">数量限制：前</span>
