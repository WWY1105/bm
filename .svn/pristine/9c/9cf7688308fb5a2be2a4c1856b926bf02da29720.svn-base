	<div class="out ruleadd2" >
		<div class="col-sm-2 text-right "></div>
		<div class="col-sm-10 content mb0">
			<div class="col-sm-12 ml25 mb22 form-control-stack text-left" >

				<label class="checkbox-inline">
					<input type="checkbox"  ng-model="posts.CHARGE.CHARGE_COUPON.check" ng-change="console(posts.CHARGE.CHARGE_COUPON.check)" ng-init="posts.CHARGE.CHARGE_COUPON.detail||(posts.CHARGE.CHARGE_COUPON.detail=temobj)" ng-checked="posts.CHARGE.CHARGE_COUPON.check">充值送券</label>
			</div>

			<div    ng-repeat="(key, detail) in posts.CHARGE.CHARGE_COUPON.detail"  ng-if="posts.CHARGE.CHARGE_COUPON.check" >
				<div class="form-inline ml25 mb22" ng-repeat="details in detail" ng-init="details.id=key">
					<select ng-model="details.id"   class="form-control "
  					ng-options="item.id as item.name for item in view.member" name="member" required></select>
					<span class=" form-control-static">充值</span>
					<input type="number" class="form-control" ng-model="details.amount" placeholder="请输入充值额度" min="0" />
					<span class=" form-control-static">元，赠送</span>

					<div class="form-group" ng-repeat="dv in details.value">
							<select class="form-control coupon-select" ng-change="ruleFn.changeCoupon(details.value, dv)" ng-model="dv.id" ng-options="item.id as item.name group by view.counponType[item.type] for item in view.coupons" required>
								<option value="">--请选择券--</option>
							</select>
							<input type="number" min="1" class="form-control" ng-model="dv.count" placeholder="请输入赠送数量" required/>
							<input type="text" ng-modle="dv.name" style="display:none;">
							<span class="form-control-static">张</span>
				</div>
				<div class="form-group">
					<a class="add click pull-left" ng-if="$parent.$first&&$first" ng-click="ruleFn.addCouponFn('CHARGE_COUPON')">
						<i class="glyphicon glyphicon-plus"></i>
					</a>
					<a class="remove click pull-left" ng-if="!$first||!$parent.$first" ng-click="ruleFn.removeFn('CHARGE_COUPON',key,$index)">
						<i class="glyphicon glyphicon-remove"></i>
					</a>
				</div>

			</div>
			<div class="form-group ml25 mb22">
				<div class="col-sm-3 btn btn-primary"  ng-click="ruleFn.openOrClose()">新增券</div>
				<div class="form-group" ng-if="isOpenOrClose" >
					<div class="col-sm-12">
						<coupon-new add-ok="ruleFn.testAddOk(result)" add-fail="ruleFn.testAddFail(result)"></coupon-new>
					</div>
				</div>
			</div>
			<div class="form-group ml25 mb22" >
					<div class="form-inline">
						<span class="form-control-static">数量限制：前</span>
						<input type="number" min="0" ng-model="posts.CHARGE.CHARGE_COUPON.countLimit" class="form-control" placeholder="请输入人数限制"/>
						<span class="form-control-static">
							人有效
							<span style="color:#999;padding-left:20px">(0表示不限制)</span>
						</span>
					</div>
			</div>
			</div>
