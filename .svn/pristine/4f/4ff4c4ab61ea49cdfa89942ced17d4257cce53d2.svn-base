<div class="doc">
  <ul class="bg-muted">
    <li>合计（元）</li>
    <li>实际收费：{{title.amount||0}}</li>
    <li>支付宝途径：{{title.alipay||0}}</li>
    <li>微信支付途径：{{title.wechatpay||0}}</li>
  </ul>
  <table class="table" ng-if="view.result.items">
    <thead>
    <tr>
      <td>序号</td>
      <td>订单号</td>
      <td>
        <div>收费金额</div>
        <div>(元)</div>
      </td>
      <td>方式</td>
      <td>操作员</td>
      <td>时间</td>
    </tr>
    </thead>
    <tbody class="slide-down">
    <tr ng-repeat="row in view.result.items">
      <td ng-bind="((page-1)*count)+$index+1"></td>
      <td ng-bind="row.orderNo||''"></td>
      <td ng-bind="row.amount||0"></td>
      <td ng-bind="row.payMode"></td>
      <td ng-bind="row.staffName||'自助'"></td>
      <td ng-bind="row.time"></td>
    </tr>
    </tbody>
  </table>
</div>
