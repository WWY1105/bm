<div class="container contents bgw">
	<div class=" col-sm-12" ng-if="view.coupons">
		<div class="col-sm-4 " ng-repeat="(key, value) in view.coupons">
			<div class="coupons" >
				<div class="title">
					<div class="col-xs-12 col-sm-12 text-center text-nowrap">
						<h3 ng-bind="value.name"></h3>
					</div>
				</div>
				<div class="content">					
						<div class="col-xs-12 " ng-repeat="(key2, value2) in value.content" ng-bind="value2">
					</div>
				</div>
			</div>
		</div>
	</div>
	<div class="row form-horizontal">
		<div class="col-sm-2 col-sm-offset-5 btn btn-danger" ng-click="openOrClose()">添加券</div>
		<div class="col-sm-offset-2 col-sm-10" ng-if="isOpenOrClose" >
				<coupon-new add-ok="testAddOk(result)" add-fail="testAddFail(result)"></coupon-new>
		</div>	
	</div>
</div>
