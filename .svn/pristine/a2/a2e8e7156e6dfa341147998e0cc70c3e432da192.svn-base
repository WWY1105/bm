<div class="doc">
  <ul class="bg-muted">
    <li>合计</li>
    <li>充值：{{title.amount||0}}元 （{{title.count}}笔）</li>
    <li>使用：{{title.usedAmount||0}}元 （{{title.usedCount}}笔）</li>
  </ul>
  <table class="table" ng-if="view.result.items">
    <thead>
    <tr>
      <td>序号</td>
      <td>分店</td>
      <td>
        <div>充值金额</div>
        <div>(元)</div>
      </td>
      <td>
        <div>充值笔数</div>
        <div>(笔)</div>
      </td>
      <td>
        <div>使用金额</div>
        <div>(元)</div>
      </td>

      <td>
        <div>使用笔数</div>
        <div>(元)</div>
      </td>
    </tr>
    </thead>
    <tbody class="slide-down">
    <tr ng-repeat="row in view.result.items">
      <td ng-bind="((page-1)*count)+$index+1"></td>
      <td ng-bind="row.shopName||'品牌虚拟店'"></td>
      <td ng-bind="row.amount||0"></td>
      <td>{{row.count||0}}</td>
      <td>- {{row.usedAmount||0}}</td>
      <td>{{row.usedCount||0}}</td>
    </tr>
    </tbody>
  </table>
</div>
