<div class="doc">
  <ul class="bg-muted">
    <li>合计（人次）</li>
    <li>充值升级：{{title.charge||0}}</li>
    <li>购买升级：{{title.buy||0}}</li>
    <li>积分升级：{{title.point||0}}</li>
    <li>领卡：{{title.free||0}}</li>
    <li class="right"><a href="#/customer">去”顾客管理“看看<span class="glyphicon glyphicon-arrow-right"></span></a></li>
  </ul>
  <!--<ul class="bg-blue">-->
    <!--<li>合计（人次）</li>-->
    <!--<li>充值升级：0</li>-->
    <!--<li>购买升级：0</li>-->
    <!--<li>积分升级：0</li>-->
    <!--<li>消费升级：0</li>-->
  <!--</ul>-->
  <table class="table" ng-if="view.result.items">
    <thead>
    <tr>
      <td>序号</td>
      <td>订单号</td>
      <td>上宾号</td>
      <td>方式</td>
      <td>升级</td>
      <td>操作员</td>
      <td>时间</td>
    </tr>
    </thead>
    <tbody class="slide-down">
    <tr ng-repeat="row in view.result.items">
      <td ng-bind="((page-1)*count)+$index+1"></td>
      <td ng-bind="row.no"></td>
      <td ng-bind="row.username"></td>
      <td ng-bind="row.reason"></td>
      <td>{{row.original+"->"+row.current}}</td>
      <td ng-bind="row.staffName||'自助'"></td>
      <td class="center" ng-bind="row.time"></td>
    </tr>
    </tbody>
  </table>
</div>


