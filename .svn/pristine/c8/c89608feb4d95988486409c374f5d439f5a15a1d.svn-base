<div class="table col-sm-12">
    <div class="col-sm-12 plan-title text-center">
      <div class="col-sm-2">门店</div>
      <div class="col-sm-2">核销数量</div>
      <div class="col-sm-2">核销金额</div>
      <div class="col-sm-2">发放数量</div>
      <div class="col-sm-2">发放金额</div>
      <div class="col-sm-2">明细</div>
    </div>

    <div class="col-sm-12 plan-son text-center slide-down" ng-repeat="row in view.result.items">
      <div class="col-sm-2" ng-bind="row.shopName||'品牌页'"></div>
      <div class="col-sm-2" ng-bind="row.amount|number"></div>
      <div class="col-sm-2" ng-bind="row.money|number"></div>
      <div class="col-sm-2" ng-bind="row.usedAmount|number"></div>
      <div class="col-sm-2" ng-bind="row.usedMoney|number"></div>
      <div class="col-sm-2" >
        <button class="btn btn-primary" ng-click="showDetail(row.shopId)">查看明细</button>
      </div>
    </div>
</div>
