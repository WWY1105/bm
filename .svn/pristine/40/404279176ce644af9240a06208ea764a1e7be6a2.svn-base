<body>
		<header ng-include="'keeper/header.html'" scope="" onload="">
		</header>
	<div class="bgb">
		<ol class=" breadcrumb container" >
			<li ng-repeat="breadCrumbs in config.breadset" ng-class="{'active':!breadCrumbs.href}" >
				<A ng-show="breadCrumbs.href" ng-href="{{breadCrumbs.href}}" ng-bind="breadCrumbs.name"></A>
				<span ng-show="!breadCrumbs.href" ng-bind="breadCrumbs.name"></span>
			</ol>

		</ol>

	</div>
	<div class="view-slide-out container  ">
		<div ng-view class="view-slide-in"></div>
	</div>

<div class="modal fade bs-example-modal-sm in" id="modal">
  <div class="modal-dialog modal-sm">
    <div class="modal-content ">
      <div class="modal-body" ng-bind="config.modalContent">

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" ng-click="console.log(config.modalLink)" data-dismiss="modal">关闭</button>
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div><!-- /.modal -->
<footer ng-include="'footer.html'" ></footer>

