<div class="doc">
  <ul class="bg-muted">
    <li>合计（条）</li>
    <li>发放：{{title.count||0}}</li>
    <li>消耗金额：{{title.amount||0}}</li>
  </ul>
  <table class="table" ng-if="view.result.items">
    <thead>
    <tr>
      <td>序号</td>
      <td>分店</td>
      <td>
        <div>发放</div>
        <div>(条)</div>
      </td>
      <td>
        <div>消耗金额</div>
        <div>(元)</div>
      </td>
    </tr>
    </thead>
    <tbody class="slide-down">
    <tr ng-repeat="row in view.result.items">
      <td ng-bind="((page-1)*count)+$index+1"></td>
      <td ng-bind="row.shopName||'品牌虚拟店'"></td>
      <td ng-bind="row.count||0"></td>
      <td ng-bind="row.amount"></td>
    </tr>
    </tbody>
  </table>
</div>




