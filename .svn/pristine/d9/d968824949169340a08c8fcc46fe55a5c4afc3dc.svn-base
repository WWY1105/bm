<div class="container contents bgw ">
	<div class="col-sm-8  col-sm-offset-2">
		<form class="form-horizontal " ng-submit="send()">
			<div class="form-group">
				<label class="control-label col-sm-4">类型</label>
				<div class="col-sm-4 ">
					<div class="form-control-static">会员</div>
				</div>
			</div>
			<div class="form-group">
				<label class="control-label col-sm-4">日期</label>
				<div class="col-sm-4">
					<div class="form-control-static" ng-bind="config.time|date:'yyyy-MM-dd'"></div>
				</div>
			</div>
			<div class="form-group ">
				<label class="control-label col-sm-4">姓名</label>
				<div class="col-sm-4">
						<input type="text" class="form-control"  ng-model="posts.fullname">
				</div>
			</div>
			<div class="form-group ">
				<label class="control-label col-sm-4">电话</label>
				<div class="col-sm-4">
						<input type="text" class="form-control"  ng-model="posts.phone">
				</div>
			</div>
			<div class="form-group ">
				<label class="control-label col-sm-4">性别</label>
				<div class="col-sm-8">
					<div class="btn-group btn-checked btn-block" data-toggle="buttons">
						<label class="btn btn-default active"  ng-click="posts.gender=0">
							<input type="radio" ng-model="posts.gender" value="0"  >未知</label>
						<label class="btn btn-default " ng-click="posts.type=1">
							<input type="radio" ng-model="posts.gender" value="1" >男</label>
						<label class="btn btn-default " ng-click="posts.type=2">
							<input type="radio" ng-model="posts.gender" value="2"  >女</label>
					</div>
				</div>
			</div>
			<div class="form-group">
				<label class="control-label col-sm-4">身份证</label>
				<div class="col-sm-4">
					<input type="text" class="form-control" ng-model="posts.id_no">
				</div>
			</div>
			<div class="form-group">
				<label class="control-label col-sm-4">生日</label>
				<div class="col-sm-4">
					<input type="date" class="form-control" ng-model="posts.birhtday">
				</div>
			</div>
			<div class="form-group">
				<div class="col-sm-2 col-sm-offset-4">
					<a class="btn btn-default btn-block" back-button>返回</a>
				</div>
				<div class="col-sm-2 ">
					<button class="btn btn-primary btn-block" >确定</button>
				</div>
				
			</div>
		</form>
	</div>
</div>