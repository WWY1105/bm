<div  class="keepershopadd" >
	<div class="container  contents bgw">

		<div class="col-sm-8 col-sm-offset-2 stepshow">
			<div class="step">
				<div class="stepover" style="width:50%"></div>
				<div class="stepson step1 active">
					<span>1</span>
					<h5>基本资料设置</h5>
				</div>
				<div class="stepson step52 active">
					<span>2</span>
					<h5>餐桌资料设置</h5>
				</div>
				<div class="stepson step2 active">
					<span>3</span>
					<h5>营业时间设置</h5>
				</div>
				<div class="stepson step54">
					<span>4</span>
					<h5>添加员工</h5>
				</div>
				<div class="stepson step3">
					<span>5</span>
					<h5>完成</h5>
				</div>
			</div>
		</div>
		<form class="row" ng-submit="postSend()" name="add_form">
			<div class="col-sm-6 col-sm-offset-3 mt20">
				
				<div class="tables">
					<div class="list-group tablenum text-center">
							<ul class="list-group-item active">
								<li class="text-center col-sm-2">时段</li>
								<li class="text-center col-sm-5">开始时间</li>
								<li class="text-center col-sm-5">结束时间</li>
							</ul>
							<ul class="list-group-item" ng-repeat="bunames in tem.buname">
								<li class="text-center col-sm-2 h44"  ng-bind="bunames">等待读取</li>
								<li class="text-center col-sm-5"  >
									<input class="form-control" type="time" ng-model="posts[$index].startTime" />
								</li>
								<li class="text-center col-sm-5"  >
									<input class="form-control" type="time" ng-model="posts[$index].endTime" />
								</li>
							</ul>
						</div>
				</div>
			</div>

			<div class="col-sm-6 col-sm-offset-3">
				<div class="row">
					<div class="col-sm-6">
						<a class="btn btn-default btn-block" ng-click="goto('shop/add2re')">返回</a>
					</div>
					<div class="col-sm-6 ">
						<button class="btn btn-primary btn-block" ng-disabled="!add_form.$valid" ng-click="sendposts()">下一步</button>
					</div>
				</div>
			</div>
		</form>
	</div>
</div>

