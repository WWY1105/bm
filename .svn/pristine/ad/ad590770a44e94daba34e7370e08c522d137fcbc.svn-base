
	<div class="container contents bgw">
		<form  name="staffadd_form"  ng-submit="staffaddsend('keeper')" >
			<div class="col-md-12">
				<div class="col-sm-10 col-sm-offset-2 form-horizontal">
					<div class="form-group">
						<label class="col-sm-2 control-label" ></label>
						<div class="col-sm-6 text-red">*为必填项</div>

					</div>
					<div class="form-group">
						<label for="shopCode" class="col-sm-2 control-label text-right">门店</label>
						<div class="col-sm-6" >
							<div class="form-control-static" ng-bind="shopsget.shop.name"></div>
						</div>
					</div>
					<div class="form-group">
						<label for="shopCode" class="col-sm-2 control-label text-right">类型</label>
						<div class="col-sm-6" >
							<div class="form-control-static" >店长</div>
						</div>
					</div>
					<div class="form-group">
						<label for="shopType" class="col-sm-2 control-label text-right">
							<span class="text-red">*</span>
							姓名
						</label>
						<div class="col-sm-6" >
							<input type="text" class="form-control" ng-model="posts.nickname" name="nickname" placeholder="请输入员工的真实姓名" ng-minlength='2' ng-maxlength='20' ng-pattern="textCheckJson.nickname" required></div>
						<span class="help-block " ng-messages="staffadd_form.nickname.$error" >
							<div  ng-message="required">2-20个字符以内</div>
							<div class="text-red" ng-message="minlength">需大于2个字符</div>
							<div  class="text-red" ng-message="maxlength">需小于20字符</div>
							<div class="text-red" ng-message="pattern">
								<span  ng-bind="textCheckErrorJson.nickname"></span>
							</div>
						</span>
						<span class="help-block" ng-show="staffadd_form.nickname.$valid">输入正确</span>
					</div>
					<div class="form-group">
						<label for="shopType" class="col-sm-2 control-label text-right">
							工号
						</label>
						<div class="col-sm-6" >
							<input type="text" class="form-control" ng-model="posts.code" name="code" placeholder="输入您的员工编号，如:cd221" ng-minlength='1' ng-maxlength='20' ng-pattern="textCheckJson.shopcode" ></div>
						<span class="help-block " ng-messages="staffadd_form.code.$error" >
							<div  ng-message="required">2-20字符的英文、数字</div>
							<div class="text-red" ng-message="minlength">需大于2个字符</div>
							<div  class="text-red" ng-message="maxlength">需小于20字符</div>
							<div class="text-red" ng-message="pattern">
								<span  ng-bind="textCheckErrorJson.name"></span>
							</div>
						
						</span>
						<span class="help-block" ng-show="staffadd_form.code.$valid">输入正确</span>
					</div>
					<div class="form-group">
						<label for="shopType" class="col-sm-2 control-label text-right">
							<span class="text-red">*</span>
							账号
						</label>
						<div class="col-sm-6" >
							<input type="text" class="form-control" ng-model="posts.name" name="name" placeholder="输入员工账号" ng-minlength='6' ng-maxlength='20' required></div>
						<span class="help-block " ng-messages="staffadd_form.name.$error" >
							<div  ng-message="required">以字母开头的6-20以内的字母、数字</div>
							<div class="text-red" ng-message="minlength">需大于6个字符</div>
							<div  class="text-red" ng-message="maxlength">需小于20字符</div>
						</span>
						<span class="help-block" ng-show="staffadd_form.name.$valid">输入正确</span>
					</div>
					<div class="form-group">
						<label for="shopType" class="col-sm-2 control-label text-right">
							<span class="text-red">*</span>
							密码
						</label>
						<div class="col-sm-6" >
							<input type="password" class="form-control" ng-model="posts.password" name="password" placeholder="请输入密码" ng-minlength='8' ng-maxlength='20' ng-pattern="textCheckJson.password" required></div>
						<span class="help-block " ng-messages="staffadd_form.password.$error" >
							<div  ng-message="required">8-20字符以内，不能含有空格</div>
							<div  class="text-red" ng-message="minlength">需大于8个字符</div>
							<div  class="text-red" ng-message="maxlength">需小于20字符</div>
							<div class="text-red" ng-message="pattern">
								<span  ng-bind="textCheckErrorJson.password"></span>
							</div>
							<div class="text-red" ng-message="reset">两次密码输入不一致</div>
						</span>
						<span class="help-block" ng-show="staffadd_form.password.$valid">输入正确</span>
					</div>
					<div class="form-group">
						<label for="shopType" class="col-sm-2 control-label text-right">
							<span class="text-red">*</span>
							重复密码
						</label>
						<div class="col-sm-6" >
							<input type="password" class="form-control" ng-model="check.repassword" name="repassword" placeholder="请输入密码" ng-minlength='8' ng-maxlength='20' ng-pattern="posts.password" required></div>
						<span class="help-block " ng-messages="staffadd_form.repassword.$error" >
							<div  ng-message="required">8-20字符以内，不能含有空格</div>
							<div  class="text-red" ng-message="minlength">需大于8个字符</div>
							<div  class="text-red" ng-message="maxlength">需小于20字符</div>
							<div class="text-red" ng-message="pattern">
								<span >两次密码输入不一致</span>
							</div>
							
						</span>
						<span class="help-block" ng-show="staffadd_form.repassword.$valid">输入正确</span>
					</div>
					<div class="col-sm-10 col-sm-offset-2">
						<div class="row">
							<div class="col-sm-3">
								<a class="btn btn-default btn-block" ng-click="goto('staff/show/'+shopId)">返回</a>
							</div>
							<div class="col-sm-3 col-sm-offset-1">
								<button type="submit" ng-disabled="!staffadd_form.$valid" class="btn btn-primary btn-block">提交</button>
							</div>
						</div>
					</div>
				</div>
			</div>
		</form>
	</div>
